package string;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;

public class AddBinaryStrings {


	public static void main(String[] args) throws IOException {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		int t = Integer.parseInt(br.readLine());
		while (t-- > 0) {
			String[] temp = br.readLine().split(" ");
			String a = temp[0];
			String b = temp[1];
			System.out.println(add(a, b));
		}
	}

	public static String add(String a, String b) {
		if (a.length() != b.length()) {
			if (a.length() > b.length()) {
				int n=a.length()-b.length();
				for (int i = 0; i < n; i++) {
					b = "0" + b;
				}
			} else {
				int n=b.length()-a.length();
				for (int i = 0; i < n; i++) {
					a = "0" + a;
				}
			}
		}
		String res = "";
		char carry = '0';
		for (int i = a.length() - 1; i >= 0; i--) {
			char ch1 = a.charAt(i);
			char ch2 = b.charAt(i);
			char sum = sum(ch1, ch2, carry);
			carry = carry(ch1, ch2, carry);
			res = sum + "" + res;
		}
		if (carry != '0')
			res = carry + res;
		return res;
	}

	private static char sum(char ch1, char ch2, char carry) {
		int a = Integer.parseInt(ch1 + "");
		int b = Integer.parseInt(ch2 + "");
		int c = Integer.parseInt(carry + "");
		if (a + b + c == 0) {
			return '0';
		} else if (a + b + c == 1) {
			return '1';
		} else if (a + b + c == 2) {
			return '0';
		} else {
			return '1';
		}
	}

	private static char carry(char ch1, char ch2, char carry) {
		int a = Integer.parseInt(ch1 + "");
		int b = Integer.parseInt(ch2 + "");
		int c = Integer.parseInt(carry + "");
		if (a + b + c == 0) {
			return '0';
		} else if (a + b + c == 1) {
			return '0';
		} else if (a + b + c == 2) {
			return '1';
		} else {
			return '1';
		}
	}
}
