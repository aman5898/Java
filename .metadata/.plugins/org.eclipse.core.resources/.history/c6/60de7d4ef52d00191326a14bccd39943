package arrays;

import java.util.*;
import java.lang.*;
import java.io.*;

class Transform {
	public static void main(String[] args) {
		Scanner scn = new Scanner(System.in);
		int t = scn.nextInt();
		while (t-- > 0) {
			int n = scn.nextInt();
			int[] arr = new int[n];
			for (int i = 0; i < arr.length; i++) {
				arr[i] = scn.nextInt();
			}
			change(arr);
			for (int val : arr) {
				System.out.print(val + " ");
			}
			System.out.println();
		}
	}

	public static void change(int[] arr) {
		boolean bl = false;
		while (bl == false) {
			bl = true;
			for (int i = 0; i < arr.length - 1; i++) {
				if (arr[i]!=0&&arr[i] == arr[i + 1]) {
					arr[i] = 2 * arr[i];
					arr[i + 1] = 0;
					i = i + 1;
					bl = false;
				} else if (arr[i + 1] == 0) {
					for (int j = i + 2; j < arr.length; j++) {
						if (arr[j] == arr[i]) {
							arr[i] = 2 * arr[i];
							arr[j] = 0;
							bl = false;
							i = j;
						} else if (arr[j] != 0) {
							break;
						}
					}
				}
			}
		}
		int i = -1;
		for (int j = 0; j < arr.length; j++) {
			if (arr[j] != 0) {
				i++;
				int temp = arr[i];
				arr[i] = arr[j];
				arr[j] = temp;
			}
		}
	}
}