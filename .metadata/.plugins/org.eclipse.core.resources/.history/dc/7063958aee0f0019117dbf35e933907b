package linkedlist;

public class client {

	public static void main(String[] args) {
		LinkedList ll=new LinkedList();
		ll.addLast(20);
		ll.addLast(30);
		ll.addLast(40);
		ll.addLast(50);
		ll.addLast(60);
		ll.addLast(70);
		ll.addLast(80);
		
		
		ll.display();
		ll.reversePI();
		ll.display();
	}

}
